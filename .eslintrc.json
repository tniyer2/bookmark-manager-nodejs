{
    "env": {
        "browser": true,
        "es6": true
    },
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": 
	{
		"for-direction": 2,
		"getter-return": 2,
		"no-await-in-loop": 2,
		"no-compare-neg-zero": 2,
		"no-cond-assign": 2,
		"no-constant-condition": 2,
		"no-control-regex": 2,
		"no-dupe-args": 2,
		"no-dupe-keys": 2,
		"no-duplicate-case": 2,
		"no-empty": 2,
		"no-empty-character-class": 2,
		"no-ex-assign": 2,
		"no-extra-boolean-cast": 2,
		"no-extra-semi": 2,
		"no-func-assign": 2,
		"no-inner-declarations": 2,
		"no-invalid-regexp": 2,
		"no-irregular-whitespace": 2,
		"no-misleading-character-class": 2,
		"no-obj-calls": 2,
		"no-regex-spaces": 2,
		"no-sparse-arrays": 2,
		"no-unexpected-multiline": 2,
		"no-unreachable": 2,
		"no-unsafe-finally": 2,
		"no-unsafe-negation": 2,
		"require-atomic-updates": 2,
		"use-isnan": 2,
		"valid-typeof": 2,

		"array-callback-return": 2,
		"dot-location": [2, "property"],
		"no-case-declarations": 2,
		"no-empty-function": 2,
		"no-eval" : 2,
		"no-extend-native": 2,
		"no-floating-decimal": 2,
		"no-global-assign": 2,
		"no-implicit-coercion": 2,
		"no-implied-eval": 2,
		"no-invalid-this": 2,
		"no-redeclare": 2,
		"no-return-assign": 2,
		"no-return-await": 2,
		"no-self-assign": 2,
		"no-self-compare": 2,
		"no-sequences": 2,
		"no-useless-return": 2,
		"yoda": 2,

		"no-shadow": 2,
		"no-undef-init": 2,
		"no-undefined": 2,

		"comma-dangle": [2, "never"],
		"consistent-this": 2,
		"eol-last": [2, "always"],
		"linebreak-style": [2, "windows"],
		"new-parens": 2,
		"no-bitwise": 2,
		"no-lonely-if": 2,
		"no-mixed-operators": 2,
		"no-plusplus": 2,
		"no-trailing-spaces": 2,
		"no-unneeded-ternary": 2,
		"no-whitespace-before-property": 2,
		"semi": [2, "always"],
		"wrap-regex": 2
	}
}